
class MyFirstClassInJava {
	public static void main(String []args) {
		System.out.println("We are just going through the basics");
		
		boolean value = false;
		
		System.out.println("value of variable is : " + value);
		
		int val  = 789;
		
		System.out.println(val);
		
		float anotherVal = 67.908F;
		
		System.out.println(anotherVal); 
		
		int intVar =  (int) anotherVal;//Error : Narrowing
		
		System.out.println(intVar);
	}

}

//	Type identifier;
	
	
	
