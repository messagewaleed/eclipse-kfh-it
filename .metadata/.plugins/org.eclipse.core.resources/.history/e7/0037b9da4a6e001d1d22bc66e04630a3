
public class MethodsWork {
	public static void main(String[] args) {
//		int result = new Calculate().add(56, 10);
		System.out.println("Addition : " + new Calculate().add(50, 181));
		System.out.println("Addition : " + new Calculate().add(50, 181, 9, 88));
	}

}

class Calculate{
	int add(int firstNum, int secondNum) {
		return firstNum + secondNum;
	}
	
	int add(int firstNum, int secondNum, int thirdNum) {
		return firstNum + secondNum + thirdNum;
	}
	
	int add(int firstNum, int secondNum, int thirdNum, int fourthNum) {
		return firstNum + secondNum + thirdNum + fourthNum;
	}
}
